{
  // Window style
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
  "window.titleSeparator": " ⚡️ ",

  // Editor style/functionality
  "editor.fontLigatures": true,
  "editor.fontFamily": "VictorMono Nerd Font, FiraCode Nerd Font, Menlo, Monaco, 'Courier New', monospace",
  "editor.formatOnType": true,

  // Git
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "git.confirmSync": false,

  // Language-specific overrides
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dart]": {
    "editor.formatOnType": true,
    // "editor.rulers": [
    //   80
    // ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.view.pdf.invert": 0.9,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.home": "/Users/calebdenio/wpilib/2021/jdk",
  "java.project.importHint": false,
  "wpilib.autoStartRioLog": false,
  "docker.dockerComposeBuild": false,
  "js/ts.implicitProjectConfig.checkJs": true,
  "remote.SSH.path": "/usr/bin/ssh",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "ruby.useLanguageServer": true,
  "vscode-pets.petType": "rubber-duck",

  // Vim
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "r", "n"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "r", "u"],
      "commands": ["code-runner.run"]
    },
    {
      "before": ["<leader>", "f", "s"],
      "commands": [":w"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": [":w"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.terminal.toggleTerminal"]
    },
    {
      "before": ["<leader>", "r", "q"],
      "commands": ["rest-client.request"]
    },
    {
      "before": ["<leader>", "d"],
      "commands": ["wpilibcore.deployCode"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "a", "c"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["<leader>", "c", "r"],
      "commands": ["workbench.action.closeEditorsToTheRight"]
    },
    {
      "before": ["<leader>", "c", "l"],
      "commands": ["workbench.action.closeEditorsToTheLeft"]
    },
    {
      "before": ["<leader>", "c", "o"],
      "commands": ["workbench.action.closeOtherEditors"]
    },
    {
      "before": ["<leader>", "c", "c"],
      "commands": [":q"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [":q"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["workbench.action.closeEditorsInGroup"]
    },
    {
      "before": ["<leader>", "g"],
      "commands": ["workbench.view.scm"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.gotoSymbol"]
    }
  ],
  "vim.camelCaseMotion.enable": true,
  "vim.replaceWithRegister": true,
  "vim.leader": " ",

  "rest-client.previewResponsePanelTakeFocus": false,
  "python.pythonPath": "/opt/homebrew/bin/python3",
  "fgh-code.promptOnOpen": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "go.toolsManagement.autoUpdate": true,
  "githubIssues.queries": [
    {
      "label": "My Issues",
      "query": "default"
    },
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
    },
    {
      "label": "Open Issues",
      "query": "state:open repo:${owner}/${repository} sort:created-desc"
    }
  ],
  "window.autoDetectColorScheme": true,
  "workbench.preferredLightColorTheme": "GitHub Light",
  "workbench.preferredDarkColorTheme": "Dracula",
  "editor.accessibilitySupport": "off",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "screencastMode.keyboardOverlayTimeout": 2000,
  "vim.neovimPath": "/opt/homebrew/bin/nvim",
  "workbench.productIconTheme": "icons-carbon",
  "clock.alignment": "Right",
  "clock.priority": -1,
  "clock.format": "hh:MM tt",
