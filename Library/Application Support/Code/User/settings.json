{
  "terminal.integrated.shell.osx": "/opt/homebrew/bin/fish",

  // Display
  "editor.fontFamily": "FiraCode Nerd Font, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "workbench.iconTheme": "vscode-icons",
  "workbench.productIconTheme": "icons-carbon",
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
  "window.titleSeparator": " ⚡️ ",

  // Git
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "git.confirmSync": false,

  "editor.formatOnSave": true,

  // Language overrides
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dart]": {
    "editor.formatOnType": true,
    // "editor.rulers": [
    //   80
    // ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.view.pdf.invert": 0.9,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.languageServer": "Microsoft",
  "java.home": "/Users/calebdenio/wpilib/2021/jdk",
  "java.project.importHint": false,
  "wpilib.autoStartRioLog": false,
  "rust-client.engine": "rust-analyzer",
  "docker.dockerComposeBuild": false,
  "js/ts.implicitProjectConfig.checkJs": true,
  "wpilib.offline": true,
  "javascript.validate.enable": false,
  "remote.SSH.path": "/usr/bin/ssh",
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "ruby.useLanguageServer": true,
  "vscode-pets.petType": "rubber-duck",

  // Vim
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "r", "n"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "f", "s"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.terminal.toggleTerminal"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["rest-client.request"]
    },
    {
      "before": ["<leader>", "d"],
      "commands": ["wpilibcore.deployCode"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [":q"]
    }
  ],
  "vim.camelCaseMotion.enable": true,
  "vim.replaceWithRegister": true,
  "vim.leader": " ",

  "rest-client.previewResponsePanelTakeFocus": false,
  "python.pythonPath": "/opt/homebrew/bin/python3",
  "fgh-code.promptOnOpen": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "go.toolsManagement.autoUpdate": true,
  "githubIssues.queries": [
    {
      "label": "My Issues",
      "query": "default"
    },
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
    },
    {
      "label": "Open Issues",
      "query": "state:open repo:${owner}/${repository} sort:created-desc"
    }
  ],
  "workbench.colorTheme": "Dracula",
  "window.autoDetectColorScheme": true,
  "workbench.preferredLightColorTheme": "GitHub Light",
  "workbench.preferredDarkColorTheme": "Dracula",
  "editor.accessibilitySupport": "off",
  "workbench.sideBar.location": "right",
  "window.zoomLevel": -1,
  "javascript.updateImportsOnFileMove.enabled": "always"
}
